{
    "name": "league/url",
    "type": "library",
    "description" : "League/url is a lightweight PHP Url manipulating library",
    "keywords": ["url", "psr-7"],
    "license": "MIT",
    "homepage": "http://url.thephpleague.com",
    "authors": [
        {
            "name" : "Ignace Nyamagana Butera",
            "email" : "nyamsprod@gmail.com",
            "homepage" : "https://github.com/nyamsprod/",
            "role" : "Developer"
        }
    ],
    "support": {
        "forum": "https://groups.google.com/forum/#!forum/thephpleague",
        "issues": "https://github.com/thephpleague/url/issues"
    },
    "require": {
        "php" : ">=5.5.0",
        "ext-mbstring" : "*",
        "ext-intl" : "*",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4.0"
    },
    "autoload": {
        "psr-4": {
            "League\\Url\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Url\\Test\\": "tests"
        }
    },
    "suggest": {
        "jeremykendall/php-domain-parser": "Parse hostname into its component subdomain, registerable domain, and public suffix parts"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
